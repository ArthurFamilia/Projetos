# Importa as bibliotecas necessárias
import json

# Caminho do arquivo JSON
caminho_arquivo = "operations_result.json"

# Lê o arquivo JSON
with open(caminho_arquivo, "r") as file:
    data = json.load(file)

# Inicializa contadores para "Gain" e "Loss"
gain_count = 0
loss_count = 0

# Itera sobre as operações no arquivo
for operation in data:
    if operation.get("Result") == "Gain":
        gain_count += 1
    elif operation.get("Result") == "Loss":
        loss_count += 1

# Exibe os resultados
print(f"Total de Gains: {gain_count}")
print(f"Total de Losses: {loss_count}")